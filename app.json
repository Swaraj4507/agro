{
  "expo": {
    "name": "Agrotech",
    "slug": "Agrotech",
    "scheme": "agrotech",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/jsk-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "newArchEnabled": false,
      "supportsTablet": true,
      "bundleIdentifier": "com.agrotech.jsk"
    },
    "android": {
      "package": "com.agrotech.jsk",
      "newArchEnabled": false,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-build-properties",
      "expo-image-picker",
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "Allow JSK Agrotech to access your Face ID biometric data."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "04edc6cc-d06f-42ad-9389-9ae8c003d43e"
      }
    }
  }
}
