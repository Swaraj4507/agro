{
  "expo": {
    "name": "Agrotech",
    "slug": "Agrotech",
    "scheme": "Agrotech",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/jsk-splash.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "package": "com.mycorp.myapp"
    },
    "android": {
      "package": "com.agrotech.jsk",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-build-properties"
    ],
    "permissions": [
      "LOCATION"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "04edc6cc-d06f-42ad-9389-9ae8c003d43e"
      }
    }
  }
}
